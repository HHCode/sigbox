head()
{
cat << EOF
Content-type: text/plain
Expires: $date

EOF
}



rear()
{
echo
    if [ "$CONTENT_LENGTH" != "" ] ; then
        if [ "$CONTENT_LENGTH" -ne 0 ] ; then
            echo 'Input:'
            echo
            dd bs=1 count=$CONTENT_LENGTH
            echo
        fi
    fi
}

parse()
{
echo "$2" | sed -n "s/^.*$1=\([^&]*\).*$/\1/p" | sed "s/%20/ /g"
}
